<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MainFormController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="450.0">
         <children>
            <Label text="Welcome to PraxNes">
               <font>
                  <Font size="32.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="100.0" top="40.0" />
               </VBox.margin>
            </Label>
            <Button mnemonicParsing="false" onAction="#onActionArticleLookup" text="Article Lookup">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onActionProgramType" text="Program Type">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onActionExit" text="Exit" />
         </children>
      </VBox>
   </children>
</AnchorPane>
